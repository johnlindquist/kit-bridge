{
  "name": "kit-bridge",
  "version": "13.0.3",
  "description": "",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "esm/*": [
        "types/*"
      ],
      "cjs/*": [
        "types/*"
      ]
    }
  },
  "exports": {
    "./esm/*": "./dist/esm/*.js",
    "./cjs/*": "./dist/cjs/*.cjs"
  },
  "files": [
    "dist/**",
    "types/**",
    "api/**"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "tsc:esm": "tsc -p tsconfig-esm.json",
    "tsc:cjs": "tsc -p tsconfig-cjs.json",
    "tsc:types": "tsc -p tsconfig-types.json",
    "build": "npm run clean && npm-run-all \"tsc:* --\" && node ./scripts/fix-cjs.mjs",
    "dev": "chokidar --initial \"./src/*.ts\" -c \"npm run build\"",
    "prepare": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^15.14.0",
    "@types/react": "^17.0.13",
    "@types/shelljs": "^0.8.8",
    "chokidar": "^3.5.2",
    "chokidar-cli": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "renamer": "^3.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "fs-extra": "^10.0.0",
    "lowdb": "^2.1.0"
  }
}
